   "source": [
    "from math import exp\n",
    "\n",
    "def proba_poisson_egal(lam, k):\n",
    "    current_term = exp(-lam)\n",
    "    cumulative_proba = current_term\n",
    "    \n",
    "    for i in range(1, k + 1):\n",
    "        current_term *= lam / i\n",
    "        if i == k:\n",
    "            return current_term\n",
    "    \n",
    "    return cumulative_proba\n",
    "\n"
   ]
  }
